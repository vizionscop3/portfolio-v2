{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,svg}", "/css/*", "/js/*"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".woff": "application/font-woff",
    ".woff2": "application/font-woff2"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block",
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.googletagmanager.com https://www.google-analytics.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https:; connect-src 'self' https://www.google-analytics.com"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "networking": {
    "allowedIpRanges": []
  },
  "forwardingGateway": {
    "allowedForwardedHosts": [
      "lifeofvizion.com",
      "www.lifeofvizion.com",
      "portfolio.lifeofvizion.com",
      "projects.lifeofvizion.com",
      "blog.lifeofvizion.com",
      "contact.lifeofvizion.com",
      "resume.lifeofvizion.com",
      "style.lifeofvizion.com",
      "docs.lifeofvizion.com",
      "media.lifeofvizion.com",
      "about.lifeofvizion.com",
      "services.lifeofvizion.com",
      "dev.lifeofvizion.com",
      "staging.lifeofvizion.com",
      "community.lifeofvizion.com",
      "newsletter.lifeofvizion.com",
      "lifestyle.lifeofvizion.com",
      "merch.lifeofvizion.com",
      "gallery.lifeofvizion.com",
      "happy-flower-01a5d2404.1.azurestaticapps.net"
    ]
  }
}
