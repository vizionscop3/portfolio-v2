# Environment Variables
# Copy this file to .env.local and update with your actual values

# Application Configuration
VITE_APP_NAME=Portfolio v2
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional portfolio website

# API Configuration (if needed)
VITE_API_BASE_URL=https://api.johndeveloper.dev
VITE_API_TIMEOUT=10000

# Analytics (Optional - replace with your actual IDs)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Error Tracking (Optional)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Social Media Links
VITE_GITHUB_URL=https://github.com/yourusername
VITE_LINKEDIN_URL=https://linkedin.com/in/yourprofile
VITE_TWITTER_URL=https://twitter.com/yourusername
VITE_EMAIL=your.email@example.com

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Development Settings
VITE_LOG_LEVEL=debug
VITE_CACHE_TTL=300000
